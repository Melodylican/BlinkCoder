<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" monitoring="autodetect" dynamicConfig="false">

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            maxElementsInMemory="100"
            eternal="false"
            timeToIdleSeconds="86400"
            timeToLiveSeconds="86400"
            overflowToDisk="true"/>

</ehcache>
